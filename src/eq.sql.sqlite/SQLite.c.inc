          }
          *(bufpt++) = (char)(exp/10+'0');             
          *(bufpt++) = (char)(exp%10+'0');             
        }
        *bufpt = 0;
        length = (int)(bufpt-buf);
        bufpt = buf;
        if( flag_zeropad && !flag_leftjustify && length < width){
          int i;
          int nPad = width - length;
          for(i=width; i>=nPad; i--){
