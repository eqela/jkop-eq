static int doubleConsonant(const char *z){
  return isConsonant(z) && z[0]==z[1];
}
static int star_oh(const char *z){
  return
    isConsonant(z) &&
    z[0]!='w' && z[0]!='x' && z[0]!='y' &&
    isVowel(z+1) &&
    isConsonant(z+2);
