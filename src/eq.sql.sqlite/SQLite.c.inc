          realvalue = -realvalue;
          prefix = '-';
        }else{
          if( flag_plussign )          prefix = '+';
          else if( flag_blanksign )    prefix = ' ';
          else                         prefix = 0;
        }
        if( xtype==etGENERIC && precision>0 ) precision--;
#if 0
        for(idx=precision, rounder=0.4999; idx>0; idx--, rounder*=0.1);
#else
        for(idx=precision, rounder=0.5; idx>0; idx--, rounder*=0.1){}
