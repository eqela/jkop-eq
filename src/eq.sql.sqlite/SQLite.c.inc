            { OP_ReadCookie,     0,         1,         BTREE_LARGEST_ROOT_PAGE},
            { OP_If,             1,         0,                 0},    
            { OP_Halt,           SQLITE_OK, OE_Abort,          0},    
            { OP_Integer,        0,         1,                 0},    
            { OP_SetCookie,      0,         BTREE_INCR_VACUUM, 1},    
